{
  "name": "patata-cli",
  "version": "5.0.0",
  "description": "Native testing made easy with CLI",
  "main": "index.js",
  "author": "Miguel Ángel Domínguez Coloma <m@eridem.net> (http://eridem.net)",
  "contributors": [],
  "license": "MIT",
  "homepage": "http://patata.io",
  "scripts": {
    "pretest": "standard --fix",
    "test": "nyc --cache mocha --timeout=8000 --check-leaks test/**/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "patata": "index.js"
  },
  "keywords": [
    "patata",
    "appium",
    "cucumberjs",
    "testing",
    "bdd",
    "android",
    "ios",
    "automation",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:eridem/patata-cli.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "standard": {
    "ignore": [
      "lib/sdk/patata.js"
    ]
  },
  "dependencies": {
    "appium": "^1.5.3",
    "asciify": "^1.3.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chalk": "^1.1.3",
    "change-case": "^3.0.0",
    "cucumber": "^1.2.2",
    "cucumber-html-reporter": "^0.2.10",
    "gherkin-lint": "^1.1.3",
    "glob": "^7.0.5",
    "hockeyapp-api-wrapper": "^0.x.x",
    "js-yaml": "^3.6.1",
    "latinize": "^0.3.0",
    "mustache": "^2.2.1",
    "require-dir": "^0.3.0",
    "shelljs": "^0.7.3",
    "wd": "^0.4.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "ghooks": "^1.3.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^8.1.0",
    "standard": "^8.0.0"
  }
}
