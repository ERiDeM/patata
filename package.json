{
  "name": "patata-cli",
  "version": "5.0.2",
  "description": "Native testing made easy",
  "main": "index.js",
  "author": {
    "name": "Miguel Ángel Domínguez Coloma",
    "email": "m@eridem.net",
    "url": "http://eridem.net"
  },
  "contributors": [],
  "license": "MIT",
  "homepage": "http://patata.io",
  "scripts": {
    "pretest": "node ./node_modules/standard/bin/cmd.js --fix",
    "test": "npm run test-coverage && npm run allowed-coverage",
    "test-coverage": "node ./node_modules/nyc/bin/nyc.js --cache mocha --timeout=8000 --check-leaks test/**/*.js",
    "allowed-coverage": "node ./node_modules/nyc/bin/nyc.js check-coverage --lines 22 --functions 23 --branches 18 test/**/*.js",
    "ideal-coverage": "node ./node_modules/nyc/bin/nyc.js check-coverage --lines 100 --functions 100 --branches 100 test/**/*.js",
    "coveralls": "node ./node_modules/nyc/bin/nyc.js report --reporter=text-lcov | node ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm test"
  },
  "preferGlobal": true,
  "bin": {
    "patata": "index.js"
  },
  "keywords": [
    "patata",
    "appium",
    "cucumberjs",
    "testing",
    "bdd",
    "android",
    "ios",
    "automation",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/eridem/patata/issues",
    "email": "m@eridem.net"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eridem/patata.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "appium": "^1.5.3",
    "asciify": "^1.3.5",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chalk": "^1.1.3",
    "cucumber": "^1.2.2",
    "cucumber-html-reporter": "^0.2.15",
    "gherkin-lint": "^1.1.3",
    "glob": "^7.1.0",
    "hockeyapp-api-wrapper": "^0.x.x",
    "js-yaml": "^3.6.1",
    "require-dir": "^0.3.0",
    "shelljs": "^0.7.4",
    "wd": "^0.4.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "ghooks": "^1.3.2",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^8.3.0",
    "standard": "^8.3.0"
  }
}