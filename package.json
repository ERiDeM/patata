{
  "name": "patata",
  "version": "6.0.2",
  "description": "Native testing made easy",
  "main": "index.js",
  "author": {
    "name": "Miguel Ángel Domínguez Coloma",
    "email": "m@eridem.net",
    "url": "http://eridem.net"
  },
  "contributors": [
    {
      "name": "Ben Bultman",
      "email": "bultman.ben@gmail.com",
      "url": "https://github.com/bbultman"
    },
    {
      "name": "Lenny Nilsson",
      "url": "https://github.com/lennynilsson"
    },
    {
      "name": "Paul Davies",
      "url": "https://github.com/pedavies"
    },
    {
      "name": "Matt Apperson",
      "url": "https://appersonlabs.com/"
    }
  ],
  "license": "MIT",
  "homepage": "http://patata.io",
  "scripts": {
    "pretest": "standard --fix",
    "test": "npm run test-coverage && npm run allowed-coverage",
    "test-coverage": "nyc --cache mocha --timeout=8000 --check-leaks test/**/*.js",
    "allowed-coverage": "nyc check-coverage --lines 25 --functions 24 --branches 21 test/**/*.js",
    "ideal-coverage": "nyc check-coverage --lines 100 --functions 100 --branches 100 test/**/*.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm test"
  },
  "preferGlobal": true,
  "bin": {
    "patata": "index.js"
  },
  "keywords": [
    "patata",
    "appium",
    "cucumberjs",
    "testing",
    "bdd",
    "android",
    "ios",
    "automation",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/eridem/patata/issues",
    "email": "m@eridem.net"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eridem/patata.git"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "appium": "^1.5.3",
    "asciify": "^1.3.5",
    "camelcase": "^4.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chalk": "^1.1.3",
    "cucumber": "^1.2.2",
    "cucumber-html-reporter": "^0.3.7",
    "gherkin-lint": "^1.1.3",
    "glob": "^7.1.1",
    "hockeyapp-api-wrapper": "^0.x.x",
    "js-yaml": "^3.7.0",
    "require-dir": "^0.3.1",
    "shelljs": "^0.7.5",
    "wd": "^0.4.0",
    "yargs": "^4.8.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.12",
    "ghooks": "^1.3.2",
    "mocha": "^3.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "nyc": "^8.3.0",
    "standard": "^8.3.0"
  }
}
